{
  "extends": "eslint:recommended",
  "rules": {
    "strict": 1,
    "quotes": [1, "single"],
    "curly": [1, "multi-line"],
    "no-use-before-define": [1, "nofunc"],
    "no-underscore-dangle": 0,
    "no-unused-vars": [2, {"vars": "local", "args": "none"}],
    "new-cap": 0,
    "consistent-return": 0,
    "camelcase": 0,
    "dot-notation": 0
  },
  env: {
    "browser": true,
    "node": true
  }
}
